{
  "name": "rehype-shiki",
  "version": "0.0.9",
  "description": "rehype plugin to highlight code blocks with shiki",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write '**/*.js' && xo --fix",
    "test": "xo",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsclarke/rehype-shiki.git"
  },
  "keywords": [
    "rehype",
    "shiki",
    "hast",
    "syntax",
    "highlight",
    "highlighting"
  ],
  "author": "Richard Clarke <rsclrk@pm.me> (https://rsclarke.dev)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rsclarke/rehype-shiki/issues"
  },
  "homepage": "https://github.com/rsclarke/rehype-shiki#readme",
  "dependencies": {
    "hast-util-to-string": "^1.0.4",
    "shiki": "^0.9.2",
    "shiki-languages": "^0.2.7",
    "unist-builder": "^2.0.3",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "typescript": "^4.1.5",
    "xo": "^0.32.1"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  }
}
